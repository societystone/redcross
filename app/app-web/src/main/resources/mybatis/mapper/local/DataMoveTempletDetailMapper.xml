<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.app.dao.local.DataMoveTempletDetailDAO">
	<resultMap id="BaseResultMap" type="DataMoveTempletDetail">
		<id column="id" property="id" jdbcType="BIGINT" />
		<result column="templet_id" property="templetId"
			jdbcType="BIGINT" />
		<result column="local_column" property="localColumn"
			jdbcType="VARCHAR" />
		<result column="remote_column" property="remoteColumn"
			jdbcType="VARCHAR" />
	</resultMap>
	<!-- 数据库列列表 -->
	<sql id="Base_Column_List">
		id, templet_id, local_column, remote_column
	</sql>

	<!-- 获得数据集合 -->
	<select id="selectList" parameterType="java.util.HashMap"
		resultMap="BaseResultMap">
		select
		<include refid="Base_Column_List" />
		from data_move_templet_detail
		where 1=1
		<if test="templetId != null">
			and templet_id=#{templetId}
		</if>
	</select>

</mapper>