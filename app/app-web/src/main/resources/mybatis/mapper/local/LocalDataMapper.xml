<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.app.dao.local.LocalDataDAO">

	<!-- 获得数据集合 -->
	<select id="selectList" statementType="PREPARED"
		parameterType="java.util.HashMap" resultType="java.util.HashMap">
		select
		<foreach collection="resultColumns" index="index" item="item"
			separator=",">
			${item}
		</foreach>
		from local_data_tbl
		where 1=1
	</select>

</mapper>